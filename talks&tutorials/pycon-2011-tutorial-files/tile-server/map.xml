<?xml version="1.0" encoding="UTF-8"?>
<Map srs="+proj=merc +a=6378137 +b=6378137 +lat_ts=0.0 +lon_0=0.0 +x_0=0.0 +y_0=0 +k=1.0 +units=m +nadgrids=@null +no_defs +over" bgcolor="#98bcda">

  <Style name="world">
    <Rule>
      <PolygonSymbolizer>
	<CssParameter name="fill">#eae8dd</CssParameter>
      </PolygonSymbolizer>
    </Rule>
  </Style>

  <Layer name="world" srs="+proj=longlat +ellps=WGS84 +datum=WGS84 +no_defs">
    <StyleName>world</StyleName>
    <Datasource>
      <Parameter name="type">shape</Parameter>
      <Parameter name="file">coastlines/10m_land</Parameter>
    </Datasource>
  </Layer>

</Map>
